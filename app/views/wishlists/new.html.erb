<% provide(:title, "New Wish Lists") %>
<h1>New Wish Lists</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_with(model: @wishlist) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>

      <%= f.label :list %>
      <%= f.text_area :list, class: 'form-control' %>

      <%= f.label :due_date %>
      <%= f.date_select :due_date, class: 'form-control' %>

      <%= f.submit "submit", class: "btn btn-primary" %>
    <% end %>
    <%= link_to "Back to Wish Lists", wishlists_path %>
  </div>
</div>